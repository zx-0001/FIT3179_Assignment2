{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Average Undernourishment % of income groups",
    "description": "Stock prices of 5 Tech Companies over Time.",
    "data": {"url": "https://raw.githubusercontent.com/zx-0001/FIT3179_Assignment2/main/data/life%20expectancy.csv"},
    "mark": {
      "type": "line"
    },
    "encoding": {
      "x": {"timeUnit": "year", "field": "Year"},
      "y": {"aggregate":"average", "field": "Prevelance of Undernourishment", "type": "quantitative"},
      "color": {"field": "IncomeGroup", "type": "nominal", "sort":["High income","Upper middle income", "Lower middle income", "Low income" ], "scale": {"range": ["#253494","#2c7fb8","#41b6c4","#a1dab4"]}},
      "tooltip": [
        { "field": "IncomeGroup", "type": "nominal", "title": "Income Group" },
        {
          "aggregate": "average",
          "field": "Prevelance of Undernourishment",
          "type": "quantitative",
          "format": ".2f",
          "title": "Average undernourishment"
        }
      ]
    }
  }
  